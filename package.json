{
  "name": "finbots-front",
  "version": "0.0.0",
  "scripts": {
    "start": "node server.js",
    "dev": "ng serve -o",
    "build:prod": "ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "format:fix": "pretty-quick --staged",
    "format:check": "prettier --config ./.prettierrc.json --list-different \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "format:all": "prettier --config ./.prettierrc.json --write \"src/{app,environments,assets}/**/*{.ts,.js,.json,.css,.scss,.html}\"",
    "logs": "heroku logs --app=finbots-success --tail"
  },
  "private": true,
  "dependencies": {
    "@angular-devkit/build-angular": "~14.2.4",
    "@angular/animations": "~12.0.4",
    "@angular/cdk": "^11.0.3",
    "@angular/cli": "~12.0.4",
    "@angular/common": "~12.0.4",
    "@angular/compiler": "~12.0.4",
    "@angular/compiler-cli": "~12.0.4",
    "@angular/core": "~12.0.4",
    "@angular/forms": "~12.0.4",
    "@angular/material": "^11.0.3",
    "@angular/platform-browser": "~12.0.4",
    "@angular/platform-browser-dynamic": "~12.0.4",
    "@angular/router": "~12.0.4",
    "@types/chart.js": "^2.9.29",
    "angular-bootstrap-md": "^11.1.0",
    "bootstrap": "^4.6.0",
    "chart.js": "^2.9.4",
    "chartist": "^0.11.4",
    "express": "^4.17.1",
    "path": "^0.12.7",
    "perfect-scrollbar": "^1.5.0",
    "quill": "^1.3.6",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "typescript": "~4.2.4",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "enhanced-resolve": "^3.3.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.3",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^1.18.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0"
  },
  "engines": {
    "node": "12.19.0",
    "npm": "7.5.3"
  }
}